/* Placeholder for missing images */
img[src=""]:not([src]),
img:not([src]),
img[src$=".webp"]:before,
img[src$=".webp"]:before {
    content: '';
    display: block;
    width: 100%;
    height: 100%;
    background: linear-gradient(135deg, #f3f4f6 0%, #e5e7eb 100%);
    position: relative;
}

/* Hide broken image icon */
img:after {
    content: attr(alt);
    display: flex;
    align-items: center;
    justify-content: center;
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: var(--bg-gray);
    color: var(--text-medium);
    font-size: 0.9rem;
    text-align: center;
    padding: 2rem;
}

